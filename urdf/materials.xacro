<?xml version="1.0" ?>
<robot name="delivbot" xmlns:xacro="http://www.ros.org/wiki/xacro" >

  <!-- Basic color palette used by visuals in RViz -->
  <material name="white">
    <color rgba="1.000 1.000 1.000 1.000"/>
  </material>

  <material name="black">
    <color rgba="0.050 0.050 0.050 1.000"/>
  </material>

  <!-- Keep silver defined for compatibility if referenced elsewhere -->
  <material name="silver">
    <color rgba="0.700 0.700 0.700 1.000"/>
  </material>

</robot>
